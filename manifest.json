{
  "name": "Injector",
  "description": "Inject Your Custom JavaScript And CSS To Any Website.",
  "version": "0.0.0.1",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["https://*.sig.ifc.edu.br/*"],
      "js": ["app.js"],
      "css": [
        "styles/main.css",
        "styles/cabecalho/cabecalho.css",
        "styles/conteudo/menu/menu.css",
        "styles/conteudo/main/index.css",
        "styles/conteudo/perfil/perfil.css"
      ]
    }
  ]
}
